{
  "name": "koa-socket-2",
  "version": "1.2.0",
  "description": "Koa meets socket.io connected socket",
  "main": "index.js",
  "scripts": {
    "test": "node spec/index.js spec/*.test.js",
    "cover": "istanbul cover --report lcovonly --print detail spec/index.js spec/*.test.js",
    "travis": "npm run cover && cat coverage/lcov.info | coveralls",
    "example": "node example/server"
  },
  "keywords": [
    "koa",
    "koa v2",
    "koa2",
    "koa version 2",
    "socket.io",
    "socket.io 2",
    "socket io",
    "socket io 2",
    "web sockets",
    "websockets",
    "websocket",
    "web socket"
  ],
  "engines": {
    "node": ">= 4"
  },
  "repository": "ambelovsky/koa-socket-2",
  "author": "Aaron Belovsky (Originally: Matt Styles)",
  "license": "MIT",
  "dependencies": {
    "koa-compose": "^4.1.0",
    "socket.io": "^2.2.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.0",
    "istanbul": "^0.4.5",
    "istanbul-harmony": "^0.3.16",
    "koa": "^2.7.0",
    "minimist": "^1.2.0",
    "socket.io-client": "^2.0.1",
    "tape": "^4.6.3"
  }
}
